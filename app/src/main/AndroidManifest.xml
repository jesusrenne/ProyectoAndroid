<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ProyectoAndroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_casa_launch_round"
        android:label="JProyectoAndroid"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HolaMundo">
        <!--Actividades o pantallas de un proyecto-->
        <activity
            android:name="com.example.ProyectoAndroid.ThirdActivity"
            android:label="@string/activity_third"
            android:exported="false" />
        <activity
            android:name="com.example.ProyectoAndroid.SecondActivity"
            android:label="@string/activity_second"
            android:exported="false" />
        <activity
            android:name="com.example.ProyectoAndroid.MainActivity"
            android:label="@string/activity_main"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>